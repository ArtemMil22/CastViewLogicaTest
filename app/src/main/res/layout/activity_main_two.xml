<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".screenOneCastView.MainActivityTwo">

    <com.example.creationownviewtest.screenOneCastView.TicTakToeView
        android:id="@+id/componentTicTakToeView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="100dp"/>

    <Button
        android:id="@+id/buttonRandomSize"
        android:layout_width="130dp"
        android:layout_height="80dp"
        android:layout_marginStart="145dp"
        android:layout_marginTop="600dp"
        android:backgroundTint="@color/greenfunny"
        android:fontFamily="casual"
        android:text="@string/update_field"
        android:textSize="18sp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginTop="680dp"
        android:backgroundTint="@color/bluedark"
        android:fontFamily="@font/aclonica"
        android:text="@string/to_next_screen" />

</FrameLayout>