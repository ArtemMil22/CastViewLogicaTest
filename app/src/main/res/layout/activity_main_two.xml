<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivityTwo">

    <com.example.creationownviewtest.TicTakToeView
        android:id="@+id/componentTicTakToeView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="100dp"/>

    <Button
        android:id="@+id/buttonRandomSize"
        android:layout_width="110dp"
        android:layout_height="80dp"
        android:layout_marginStart="165dp"
        android:layout_marginTop="600dp"
        android:backgroundTint="#4CAF50"
        android:fontFamily="casual"
        android:text="random SIZE"
        android:textSize="18sp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="75dp"
        android:layout_marginTop="680dp"
        android:backgroundTint="#3F51B5"
        android:fontFamily="@font/aclonica"
        android:text="Перейти на следующий экран" />

</FrameLayout>